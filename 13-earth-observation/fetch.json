{
    "conditions": {
        "executortype": "container-executor",
        "executornames": [
            "edge"
        ],
        "nodes": 1,
        "processespernode": 1,
        "mem": "10Gi",
        "cpu": "1000m",
        "gpu": {
            "count": 0
        },
        "walltime": 60
    },
    "funcname": "execute",
    "kwargs": {
        "cmd": "python3",
        "docker-image": "johan/openeo",
		"args": [
            "/cfs/src/fetch.py"
		]
    },
	"env": {
		"aoi": "{\"east\": 22.136334940987638, \"south\": 65.63560585326866, \"west\": 21.79257719972466, \"north\": 65.78212032521256}",
        "temporal_extent": "[\"2024-08-02T00:00:00Z\", \"2024-08-10T10:00:00Z\"]",
        "bands": "[\"b04\", \"b03\", \"b02\", \"b08\"]",
		"label": "/openeo/rutvik",
		"openeourl": "https://openeo.digitalearth.se",
		"openeouser": "XXX",
		"openeopasswd": "XXX"

	},
	"fs": {
        "mount": "/cfs",
        "dirs": [
            {
                "label": "/openeo/src",
                "dir": "/src",
                "keepfiles": false,
                "onconflicts": {
                    "onstart": {
                        "keeplocal": false
                    },
                    "onclose": {
                        "keeplocal": false 
                    }
                }
            },
		    {
                 "label": "/openeo/rutvik",
                 "dir": "/openeo/rutvik",
                 "keepfiles": false,
                 "onconflicts": {
                    "onstart": {
                         "keeplocal": false
                 	},
                    "onclose": {
                    	"keeplocal": true
           			 }
				 }
			}
        ]
    },
    "maxexectime": 55,
    "maxretries": 3
}
